# Completion for Zanata Java command-line client
# Generated by BashCompletionGenerator
#
_zanata()
{
    local cur prev opts base cmds
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    base="${COMP_WORDS[1]}"
    cmds="init list-remote pull push put-project put-user put-version stats glossary-delete glossary-push glossary-pull"
    if [[ ${#COMP_WORDS[@]} == 2 ]] ; then
        COMPREPLY=( $(compgen -W "${cmds} --help" -- ${cur}) )
        return 0
    fi
    if [[ ${COMP_WORDS[1]} == '--help' ]] ; then
        COMPREPLY=( $(compgen -W "${cmds}" -- ${cur}) )
        return 0
    fi
    case "${prev}" in
        --config)
            COMPREPLY=( $(compgen -df ${cur}) )
            return 0
            ;;
        --project-config)
            COMPREPLY=( $(compgen -df ${cur}) )
            return 0
            ;;
        --user-config)
            COMPREPLY=( $(compgen -df ${cur}) )
            return 0
            ;;
        --cache-dir)
            COMPREPLY=( $(compgen -d ${cur}) )
            return 0
            ;;
        --src-dir)
            COMPREPLY=( $(compgen -d ${cur}) )
            return 0
            ;;
        --trans-dir)
            COMPREPLY=( $(compgen -d ${cur}) )
            return 0
            ;;
        --source-checkout-url)
            COMPREPLY=( $(compgen -A hostname ${cur}) )
            return 0
            ;;
        --source-view-url)
            COMPREPLY=( $(compgen -A hostname ${cur}) )
            return 0
            ;;
        --url)
            COMPREPLY=( $(compgen -A hostname ${cur}) )
            return 0
            ;;
    esac
    case "${base}" in
        glossary-delete)
            local glossary-delete_opts="--id --all --project --config --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${glossary-delete_opts}" -- ${cur}) )
            return 0
            ;;
        glossary-pull)
            local glossary-pull_opts="--file-type --trans-lang --project --config --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${glossary-pull_opts}" -- ${cur}) )
            return 0
            ;;
        glossary-push)
            local glossary-push_opts="--file --batch-size --trans-lang --project --config --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${glossary-push_opts}" -- ${cur}) )
            return 0
            ;;
        init)
            local init_opts="--includes --excludes --project-type --project --project-config --project-version --src-dir --trans-dir --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${init_opts}" -- ${cur}) )
            return 0
            ;;
        list-remote)
            local list-remote_opts="--includes --excludes --project-type --project --project-config --project-version --src-dir --trans-dir --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${list-remote_opts}" -- ${cur}) )
            return 0
            ;;
        pull)
            local pull_opts="--locales --pull-type --create-skeletons --encode-tabs --include-fuzzy --continue-after-error --min-doc-percent --use-cache --purge-cache --cache-dir --from-doc --dry-run --includes --excludes --project-type --project --project-config --project-version --src-dir --trans-dir --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${pull_opts}" -- ${cur}) )
            return 0
            ;;
        push)
            local push_opts="--case-sensitive --validate --locales --src-lang --copy-trans --merge-type --push-type --default-excludes --chunk-size --file-types --list-file-types --exclude-locale-filenames --my-trans --from-doc --dry-run --includes --excludes --project-type --project --project-config --project-version --src-dir --trans-dir --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${push_opts}" -- ${cur}) )
            return 0
            ;;
        put-project)
            local put-project_opts="--project-slug --project-name --project-desc --source-view-url --source-checkout-url --default-project-type --project-status --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${put-project_opts}" -- ${cur}) )
            return 0
            ;;
        put-user)
            local put-user_opts="--user-name --user-email --user-username --user-passwordhash --user-key --user-langs --user-roles --user-enabled --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${put-user_opts}" -- ${cur}) )
            return 0
            ;;
        put-version)
            local put-version_opts="--project-type --version-project --version-slug --version-status --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${put-version_opts}" -- ${cur}) )
            return 0
            ;;
        stats)
            local stats_opts="--format --details --word --docid --includes --excludes --project-type --project --project-config --project-version --src-dir --trans-dir --url --username --key --user-config --log-http --disable-ssl-cert --debug --batch-mode --quiet --errors --help"
            COMPREPLY=( $(compgen -W "${stats_opts}" -- ${cur}) )
            return 0
            ;;
    esac
}
complete -F _zanata zanata-cli